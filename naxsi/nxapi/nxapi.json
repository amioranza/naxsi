{
"elastic" : {
 "host" : "es-layer:9200",
 "use_ssl" : false,
 "index" : "nxapi",
 "doctype" : "events",
 "default_ttl" : "7200",
 "max_size" : "1000",
 "version" : "2"
},
"syslogd": {
 "host" : "0.0.0.0",
 "port" : "51400"
},
"global_filters" : {
 "whitelisted" : "false"
},
"global_warning_rules" : {
 "rule_ip" : ["<=", 10 ],
 "global_rule_ip_ratio" : ["<", 5]
},
"global_success_rules" : {
 "global_rule_ip_ratio" : [">=", 10],
 "rule_ip" : [">=", 10]
},
"global_deny_rules" : {
 "global_rule_ip_ratio" : ["<", 2]
},
"naxsi" : {
 "rules_path" : "/etc/naxsi/naxsi_core.rules",
 "template_path" : [ "tpl/"],
 "geoipdb_path" : "/opt/nxapi/nx_datas/country2coords.txt"
},
"output" : {
 "colors" : "true",
 "verbosity" : "5"
}
}
